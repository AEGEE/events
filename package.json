{
  "name": "events",
  "version": "1.1.1",
  "description": "Events module of MyAEGEE",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run db:setup && jest test/api/*.js --runInBand --forceExit",
    "lint": "node_modules/.bin/eslint .",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "open-coverage": "open-cli coverage/lcov-report/index.html",
    "cli": "node --experimental-repl-await -r ./cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js"
    ]
  },
  "author": "Nico Westerbeck",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AEGEE/events/issues"
  },
  "homepage": "https://github.com/AEGEE/events#readme",
  "dependencies": {
    "@bugsnag/js": "^6.5.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "express-query-boolean": "^2.0.0",
    "faker": "^5.1.0",
    "file-type": "^14.6.2",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-schedule": "^1.3.2",
    "node-xlsx": "^0.15.0",
    "on-finished": "^2.3.0",
    "pg": "^8.3.3",
    "prom-client": "^12.0.0",
    "read-chunk": "^3.2.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rimraf": "^3.0.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "codecov": "^3.7.2",
    "commitlint": "^11.0.0",
    "conventional-changelog": "^3.1.23",
    "conventional-changelog-cli": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "lint-staged": "^10.2.11",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "opn-cli": "^5.0.0",
    "semantic-release": "^17.1.2",
    "timekeeper": "^2.2.0"
  }
}
